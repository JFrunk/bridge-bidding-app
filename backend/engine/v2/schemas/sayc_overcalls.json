        "hcp": {
          "min": 12,
          "max": 16
        },
        "suit_length": {
          "min": 5
        },
        "suit_quality": {
          "min": "excellent"
        },
        "suit_hcp": {
          "min": 5
        },
        "quick_tricks": {
          "min": 1.5
        },
        "partnership_hcp_min": 20,
        "opponent_suit_length": {
          "max": 3
        },
        "bid_level": 2
