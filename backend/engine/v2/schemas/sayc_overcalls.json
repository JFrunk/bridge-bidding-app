    {
      "id": "pass_no_action",
      "bid": "Pass",
      "priority": 11,
      "description": "Pass in competitive auction",
      "conditions": {
        "is_overcall": true,
        "OR": [
          {
            "hcp": {
              "max": 7
            }
          },
          {
            "suit_length": {
              "max": 4
            }
          }
        ]
      },
      "forcing": "none",
      "explanation": "Pass without sufficient values for action",
      "sets_forcing_level": "NON_FORCING"
    }
  ]
}