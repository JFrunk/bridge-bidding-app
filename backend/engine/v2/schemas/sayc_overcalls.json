{
  "schema_version": "1.0",
  "system": "SAYC",
  "category": "overcalls",
  "description": "Overcalls and competitive bidding - direct seat actions",
  "rules": [
    {
      "id": "1nt_overcall",
      "bid": "1NT",
      "priority": 800,
      "description": "Direct 1NT overcall (15-18 HCP balanced with stopper)",
      "constraints": [
        {"feature": "is_overcall", "expected": true, "constraint_type": "HARD"},
        {"feature": "is_balancing", "expected": false, "constraint_type": "HARD"},
        {"feature": "my_bid_count", "max": 0, "constraint_type": "HARD"},
        {"feature": "hcp", "min": 15, "max": 18, "constraint_type": "SOFT", "penalty_per_unit": 0.1},
        {"feature": "is_balanced", "expected": true, "constraint_type": "HARD"},
        {"feature": "stopper_in_opponent_suit", "expected": true, "constraint_type": "HARD"}
      ],
      "forcing": "none",
      "explanation": "1NT overcall showing {hcp} HCP balanced with stopper in opponent's suit",
      "sets_forcing_level": "NON_FORCING"
    },
    {
      "id": "overcall_1_level_spades",
      "bid": "1♠",
      "priority": 720,
      "description": "1♠ overcall with 5+ spades",
      "constraints": [
        {"feature": "is_overcall", "expected": true, "constraint_type": "HARD"},
        {"feature": "is_balancing", "expected": false, "constraint_type": "HARD"},
        {"feature": "my_bid_count", "max": 0, "constraint_type": "HARD"},
        {"feature": "hcp", "min": 8, "max": 16, "constraint_type": "SOFT", "penalty_per_unit": 0.1},
        {"feature": "spades_length", "min": 5, "constraint_type": "HARD"},
        {"feature": "spades_quality", "min": 2, "constraint_type": "SOFT", "penalty_per_unit": 0.2}
      ],
      "forcing": "none",
      "explanation": "1♠ overcall with {hcp} HCP and 5+ spades",
      "sets_forcing_level": "NON_FORCING"
    },
    {
      "id": "overcall_1_level_hearts",
      "bid": "1♥",
      "priority": 715,
      "description": "1♥ overcall with 5+ hearts",
      "constraints": [
        {"feature": "is_overcall", "expected": true, "constraint_type": "HARD"},
        {"feature": "is_balancing", "expected": false, "constraint_type": "HARD"},
        {"feature": "my_bid_count", "max": 0, "constraint_type": "HARD"},
        {"feature": "hcp", "min": 8, "max": 16, "constraint_type": "SOFT", "penalty_per_unit": 0.1},
        {"feature": "hearts_length", "min": 5, "constraint_type": "HARD"},
        {"feature": "hearts_quality", "min": 2, "constraint_type": "SOFT", "penalty_per_unit": 0.2}
      ],
      "forcing": "none",
      "explanation": "1♥ overcall with {hcp} HCP and 5+ hearts",
      "sets_forcing_level": "NON_FORCING"
    },
    {
      "id": "overcall_1_level_diamonds",
      "bid": "1♦",
      "priority": 700,
      "description": "1♦ overcall with 5+ diamonds",
      "constraints": [
        {"feature": "is_overcall", "expected": true, "constraint_type": "HARD"},
        {"feature": "is_balancing", "expected": false, "constraint_type": "HARD"},
        {"feature": "my_bid_count", "max": 0, "constraint_type": "HARD"},
        {"feature": "hcp", "min": 8, "max": 16, "constraint_type": "SOFT", "penalty_per_unit": 0.1},
        {"feature": "diamonds_length", "min": 5, "constraint_type": "HARD"},
        {"feature": "diamonds_quality", "min": 2, "constraint_type": "SOFT", "penalty_per_unit": 0.2}
      ],
      "forcing": "none",
      "explanation": "1♦ overcall with {hcp} HCP and 5+ diamonds",
      "sets_forcing_level": "NON_FORCING"
    },
    {
      "id": "overcall_2_level_suit",
      "bid": "2{longest_suit}",
      "priority": 680,
      "description": "2-level suit overcall (needs good values)",
      "constraints": [
        {"feature": "is_overcall", "expected": true, "constraint_type": "HARD"},
        {"feature": "is_balancing", "expected": false, "constraint_type": "HARD"},
        {"feature": "my_bid_count", "max": 0, "constraint_type": "HARD"},
        {"feature": "hcp", "min": 10, "max": 16, "constraint_type": "SOFT", "penalty_per_unit": 0.1},
        {"feature": "longest_suit_length", "min": 5, "constraint_type": "HARD"},
        {"feature": "longest_suit_quality", "min": 2, "constraint_type": "SOFT", "penalty_per_unit": 0.15}
      ],
      "forcing": "none",
      "explanation": "2-level overcall in {longest_suit} with {hcp} HCP",
      "sets_forcing_level": "NON_FORCING"
    },
    {
      "id": "1nt_overcall_balancing",
      "bid": "1NT",
      "priority": 94,
      "description": "Balancing 1NT",
      "conditions": {
        "is_overcall": true,
        "is_balancing": true,
        "hcp": {
          "min": 12,
          "max": 15
        },
        "is_balanced": true,
        "stopper_in_opponent_suit": true
      },
      "forcing": "none",
      "explanation": "Balancing 1NT showing {hcp} HCP (3 less than direct)",
      "sets_forcing_level": "NON_FORCING"
    },
    {
      "id": "michaels_cuebid_major",
      "bid": "{opponent_suit}",
      "priority": 600,
      "description": "Michaels cuebid over major",
      "conditions": {
        "is_overcall": true,
        "opening_bid": {
          "in": [
            "1\u2665",
            "1\u2660"
          ]
        },
        "hcp": {
          "min": 8,
          "max": 16
        },
        "other_major_length": {
          "min": 5
        },
        "any_minor_length": {
          "min": 5
        }
      },
      "forcing": "one_round",
      "explanation": "Michaels cuebid showing 5-5 in other major and a minor",
      "sets_forcing_level": "FORCING_1_ROUND"
    },
    {
      "id": "michaels_cuebid_minor",
      "bid": "{opponent_suit}",
      "priority": 609,
      "description": "Michaels cuebid over minor",
      "conditions": {
        "is_overcall": true,
        "opening_bid": {
          "in": [
            "1\u2663",
            "1\u2666"
          ]
        },
        "hcp": {
          "min": 8,
          "max": 16
        },
        "spades_length": {
          "min": 5
        },
        "hearts_length": {
          "min": 5
        }
      },
      "forcing": "one_round",
      "explanation": "Michaels cuebid showing 5-5 in the majors",
      "sets_forcing_level": "FORCING_1_ROUND"
    },
    {
      "id": "unusual_2nt",
      "bid": "2NT",
      "priority": 85,
      "description": "Unusual 2NT",
      "conditions": {
        "is_overcall": true,
        "opening_bid": {
          "in": [
            "1\u2665",
            "1\u2660"
          ]
        },
        "hcp": {
          "min": 6,
          "max": 15
        },
        "clubs_length": {
          "min": 5
        },
        "diamonds_length": {
          "min": 5
        }
      },
      "forcing": "one_round",
      "explanation": "Unusual 2NT showing 5-5 in the minors",
      "sets_forcing_level": "FORCING_1_ROUND"
    },
    {
      "id": "simple_overcall_1_level",
      "bid": "1{suit}",
      "priority": 90,
      "description": "Simple 1-level overcall",
      "conditions": {
        "is_overcall": true,
        "opening_bid": {
          "in": [
            "1\u2663",
            "1\u2666"
          ]
        },
        "hcp": {
          "min": 9,
          "max": 16
        },
        "suit_length": {
          "min": 5
        },
        "bid_higher_than_opening": true
      },
      "forcing": "none",
      "suit_selection": "best_5_card_suit",
      "explanation": "Simple overcall with {hcp} HCP and {suit_length}-card {suit}",
      "sets_forcing_level": "NON_FORCING"
    },
    {
      "id": "simple_overcall_1s_over_1h",
      "bid": "1\u2660",
      "priority": 95,
      "description": "1♠ overcall over 1♥",
      "conditions": {
        "is_overcall": true,
        "opening_bid": "1\u2665",
        "hcp": {
          "min": 9,
          "max": 16
        },
        "spades_length": {
          "min": 5
        }
      },
      "forcing": "none",
      "explanation": "1♠ overcall showing {hcp} HCP and 5+ spades",
      "sets_forcing_level": "NON_FORCING"
    },
    {
      "id": "simple_overcall_1_level_balancing",
      "bid": "1{suit}",
      "priority": 99,
      "description": "Balancing 1-level overcall (8-14 HCP, stronger hands double)",
      "conditions": {
        "is_overcall": true,
        "is_balancing": true,
        "hcp": {
          "min": 8,
          "max": 14
        },
        "suit_length": {
          "min": 5
        },
        "suit_quality": {
          "min": "good"
        },
        "suit_hcp": {
          "min": 3
        },
        "partnership_hcp_min": 18
      },
      "forcing": "none",
      "suit_selection": "best_5_card_suit",
      "explanation": "Balancing overcall with {hcp} HCP and good {suit_length}-card {suit}",
      "sets_forcing_level": "NON_FORCING"
    },
    {
      "id": "simple_overcall_2_level_strong",
      "bid": "2{suit}",
      "priority": 96,
      "description": "Strong 2-level overcall with excellent suit",
      "conditions": {
        "is_overcall": true,
        "hcp": {
          "min": 13,
          "max": 16
        },
        "suit_length": {
          "min": 6
        },
        "suit_quality": {
          "min": "excellent"
        },
        "suit_hcp": {
          "min": 6
        },
        "quick_tricks": {
          "min": 2
        },
        "partnership_hcp_min": 20,
        "opponent_suit_length": {
          "max": 2
        },
        "bid_level": 2
      },
      "forcing": "none",
      "suit_selection": "best_6_card_suit",
      "explanation": "2-level overcall showing 13-16 HCP with excellent 6+ card suit and 6+ HCP in suit",
      "sets_forcing_level": "NON_FORCING"
    },
    {
      "id": "simple_overcall_2_level",
      "bid": "2{suit}",
      "priority": 95,
