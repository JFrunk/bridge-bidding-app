      "sets_forcing_level": "NON_FORCING"
    },
    {
      "id": "weak_jump_overcall",
      "bid": "2{suit}",
      "priority": 90,
      "description": "Weak jump overcall",
      "conditions": {
        "is_overcall": true,
        "opening_bid": {
          "in": [
            "1\u2663",
            "1\u2666",
            "1\u2665"
          ]
        },
        "hcp": {
          "min": 6,
          "max": 10
        },
        "suit_length": {
          "min": 6
        },
        "suit_quality": {
          "min": "fair"
        },
        "is_jump": true
      },
      "forcing": "none",
      "explanation": "Weak jump overcall with {hcp} HCP and 6-card {suit}",
      "sets_forcing_level": "NON_FORCING"
    },
    {
      "id": "negative_double",
      "bid": "X",
      "priority": 800,
      "description": "Negative double by responder",
      "conditions": {
        "is_response": true,
        "interference_present": true,
        "hcp": {
          "min": 6
        },
        "support_unbid_major": true
      },
      "forcing": "one_round",
      "explanation": "Negative double showing unbid major(s)",
      "sets_forcing_level": "FORCING_1_ROUND"
    },
    {
      "id": "competitive_later_pass_weak",
      "bid": "Pass",
      "priority": 15,
      "description": "Pass in later competitive round with weak hand",
      "conditions": {
        "is_competitive_later": true,
        "hcp": {
          "max": 11
        }
      },
      "forcing": "none",
      "explanation": "Pass with only {hcp} HCP (already acted in competition)",
      "sets_forcing_level": "NON_FORCING"
    },
    {
      "id": "competitive_later_pass_no_fit",
      "bid": "Pass",
      "priority": 14,
      "description": "Pass in later competitive round with no fit",
      "conditions": {
        "is_competitive_later": true,
        "partner_suit_length": {
          "max": 2
        },
        "hcp": {
          "max": 14
        }
      },
      "forcing": "none",
      "explanation": "Pass with no fit for partner's suit",
      "sets_forcing_level": "NON_FORCING"
    },
    {
      "id": "competitive_later_pass_minimum",
      "bid": "Pass",
      "priority": 13,
      "description": "Pass in later competitive round with minimum values",
      "conditions": {
        "is_competitive_later": true,
        "hcp": {
          "max": 15
        },
        "suit_length": {
          "max": 5
        }
      },
      "forcing": "none",
      "explanation": "Pass with minimum values for further action",
      "sets_forcing_level": "NON_FORCING"
    },
    {
      "id": "competitive_later_pass_default",
      "bid": "Pass",
      "priority": 10,
      "description": "Default pass in later competitive rounds",
      "conditions": {
        "is_competitive_later": true
      },
      "forcing": "none",
      "explanation": "Pass (already competed, nothing further to show)",
      "sets_forcing_level": "NON_FORCING"
    },
    {
      "id": "pass_trap",
      "bid": "Pass",
      "priority": 50,
      "description": "Trap pass with length in opponent's suit",
      "conditions": {
        "is_overcall": true,
        "hcp": {
          "min": 10,
